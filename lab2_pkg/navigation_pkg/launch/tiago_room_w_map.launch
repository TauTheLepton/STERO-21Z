<launch>
    <!-- declare arg to be passed in -->
    <arg name="linear" default="0.09"/> 
    <arg name="angular" default="0.08"/> 
    <arg name="multiplier" default="1"/> 
    <arg name="direction" default="1"/> 
    <arg name="odom" default="False"/> 
    <arg name="delay" default="10"/> 

    <param name="/use_sim_time" value="True" />

    <node name="map_server" pkg="map_server" type="map_server" args="$(find navigation_pkg)/maps/moja_mapa.yaml" />

    <include file="$(find stero_mobile_init)/launch/tiago_simple.launch" >
        <arg name="world" value="moj_swiat" />
    </include>


</launch>
